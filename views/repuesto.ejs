    .preloader {
      align-items: center;
      justify-content: center;
      text-align: center;
      max-width: 20em;
      width: 100%;
    }

    .preloader__text {
      position: relative;
      height: 1.5em;
    }

    .preloader__msg {
      color: white;
      animation: msg 0.3s 13.7s linear forwards;
      position: absolute;
      width: 100%;
    }

    .preloader__msg--last {
      color: white;
      animation-direction: reverse;
      animation-delay: 14s;
      visibility: hidden;
    }

    .cart {
      display: block;
      margin: 0 auto 1.5em auto;
      width: 12em;
      height: 12em;
    }

    .cart__lines,
    .cart__top,
    .cart__wheel1,
    .cart__wheel2,
    .cart__wheel-stroke {
      animation: cartLines 2s ease-in-out infinite;
    }

    .cart__lines {
      stroke: var(--primary);
    }

    .cart__top {
      animation-name: cartTop;
    }

    .cart__wheel1 {
      animation-name: cartWheel1;
      transform: rotate(-0.25turn);
      transform-origin: 43px 111px;
   
    }
    .daki{
      animation: myAnim2 2s cubic-bezier(0.32, 0, 0.67, 0) 0s infinite  normal forwards;
    }
    .daki:hover{
      animation: myAnim 2s cubic-bezier(0.32, 0, 0.67, 0) 0s infinite  normal forwards;
    }
   
    @keyframes myAnim {
	0% {
    animation-timing-function: ease-out;
		
    transform: rotate(0deg);
	}

	38% {
    animation-timing-function:  ease-in;
    transform: rotate(45deg);
  
	}

	55% {
    animation-timing-function: ease-out;
		transform: rotate(0deg);
	}

	72% {
    animation-timing-function:  ease-in;
    transform: rotate(-45deg);
	}

	

	95% {
    animation-timing-function: ease-out;
    transform: rotate(0deg);
	}

	100% {
    animation-timing-function:  ease-in;
    transform: rotate(0deg);
	}
}

@keyframes myAnim2 {

	0% {
		animation-timing-function: ease-out;
		opacity: 1;
		transform: translateY(0);
	}

	25% {
		animation-timing-function: ease-in;
		transform: translateY(-65px);
	}

	50% {
		animation-timing-function: ease-out;
		transform: translateY(0);
	}

	75% {
		animation-timing-function: ease-in;
		transform: translateY(-28px);
	}

	90% {
		animation-timing-function: ease-out;
		transform: translateY(0);
	}

	95% {
		animation-timing-function: ease-in;
		transform: translateY(-8px);
	}

	100% {
		animation-timing-function: ease-out;
		transform: translateY(0);
	}
}
